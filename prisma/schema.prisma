model pages_rels {
  id                                Int           @id @default(autoincrement())
  order                             Int?
  parent_id                         Int
  path                              String        @db.VarChar
  pages_id                          Int?
  testimonials_id                   Int?
  jobs_id                           Int?
  jobs                              jobs?         @relation(fields: [jobs_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_rels_jobs_fk")
  pages_pages_rels_pages_idTopages  pages?        @relation("pages_rels_pages_idTopages", fields: [pages_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_rels_pages_fk")
  pages_pages_rels_parent_idTopages pages         @relation("pages_rels_parent_idTopages", fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_rels_parent_fk")
  testimonials                      testimonials? @relation(fields: [testimonials_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_rels_testimonials_fk")

  @@index([jobs_id])
  @@index([order])
  @@index([pages_id])
  @@index([parent_id], map: "pages_rels_parent_idx")
  @@index([path])
  @@index([testimonials_id])
}

model payload_locked_documents {
  id                            Int                             @id @default(autoincrement())
  global_slug                   String?                         @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([global_slug])
  @@index([updated_at])
}

model payload_locked_documents_rels {
  id                       Int                      @id @default(autoincrement())
  order                    Int?
  parent_id                Int
  path                     String                   @db.VarChar
  media_id                 Int?
  users_id                 Int?
  navigation_menu_id       Int?
  nav_links_id             Int?
  social_links_id          Int?
  pages_id                 Int?
  skills_collection_id     Int?
  skills_id                Int?
  testimonials_id          Int?
  jobs_id                  Int?
  projects_id              Int?
  project_tags_id          Int?
  project_pages_id         Int?
  jobs                     jobs?                    @relation(fields: [jobs_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "payload_locked_documents_rels_jobs_fk")
  media                    media?                   @relation(fields: [media_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "payload_locked_documents_rels_media_fk")
  nav_links                nav_links?               @relation(fields: [nav_links_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "payload_locked_documents_rels_nav_links_fk")
  navigation_menu          navigation_menu?         @relation(fields: [navigation_menu_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "payload_locked_documents_rels_navigation_menu_fk")
  pages                    pages?                   @relation(fields: [pages_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "payload_locked_documents_rels_pages_fk")
