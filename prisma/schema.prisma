generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URI")
}

model jobs {
  id                            Int                             @id @default(autoincrement())
  company_name                  String                          @db.VarChar
  job_role                      String                          @db.VarChar
  start_date                    DateTime                        @db.Timestamptz(3)
  current_position              Boolean?
  end_date                      DateTime?                       @db.Timestamptz(3)
  location                      String                          @db.VarChar
  position_type                 enum_jobs_position_type
  company_description           String                          @db.VarChar
  company_website               String                          @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  jobs_blocks_image_block       jobs_blocks_image_block[]
  jobs_blocks_rich_text_block   jobs_blocks_rich_text_block[]
  jobs_rels                     jobs_rels[]
  pages_rels                    pages_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model jobs_blocks_image_block {
  order           Int                                           @map("_order")
  parent_id       Int                                           @map("_parent_id")
  path            String                                        @map("_path")
  id              String                                        @id @db.VarChar
  image_id        Int
  gradient        Boolean?                                      @default(false)
  gradient_x_flip Boolean?                                      @default(false)
  gradient_y_flip Boolean?                                      @default(false)
  gradient_select enum_jobs_blocks_image_block_gradient_select?
  block_name      String?                                       @db.VarChar
  forced_width    Decimal                                       @default(0) @db.Decimal
  forced_height   Decimal                                       @default(0) @db.Decimal
  media           media                                         @relation(fields: [image_id], references: [id], onDelete: SetNull, onUpdate: NoAction, map: "jobs_blocks_image_block_image_id_media_id_fk")
  jobs            jobs                                          @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "jobs_blocks_image_block_parent_id_fk")

  @@index([image_id], map: "jobs_blocks_image_block_image_idx")
  @@index([order], map: "jobs_blocks_image_block_order_idx")
  @@index([parent_id], map: "jobs_blocks_image_block_parent_id_idx")
  @@index([path], map: "jobs_blocks_image_block_path_idx")
}

model jobs_blocks_rich_text_block {
  order        Int     @map("_order")
  parent_id    Int     @map("_parent_id")
  path         String  @map("_path")
  id           String  @id @db.VarChar
  content      Json
  content_html String? @db.VarChar
  block_name   String? @db.VarChar
  jobs         jobs    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "jobs_blocks_rich_text_block_parent_id_fk")

  @@index([order], map: "jobs_blocks_rich_text_block_order_idx")
  @@index([parent_id], map: "jobs_blocks_rich_text_block_parent_id_idx")
  @@index([path], map: "jobs_blocks_rich_text_block_path_idx")
}

model jobs_rels {
  id        Int     @id @default(autoincrement())
  order     Int?
  parent_id Int
  path      String  @db.VarChar
  skills_id Int?
  jobs      jobs    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "jobs_rels_parent_fk")
  skills    skills? @relation(fields: [skills_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "jobs_rels_skills_fk")

  @@index([order])
  @@index([parent_id], map: "jobs_rels_parent_idx")
  @@index([path])
  @@index([skills_id])
}

model media {
  id                               Int                                @id @default(autoincrement())
  alt                              String                             @db.VarChar
  updated_at                       DateTime                           @default(now()) @db.Timestamptz(3)
  created_at                       DateTime                           @default(now()) @db.Timestamptz(3)
  url                              String?                            @db.VarChar
  thumbnail_u_r_l                  String?                            @db.VarChar
  filename                         String?                            @unique(map: "media_filename_idx") @db.VarChar
  mime_type                        String?                            @db.VarChar
  filesize                         Decimal?                           @db.Decimal
  width                            Decimal?                           @db.Decimal
  height                           Decimal?                           @db.Decimal
  focal_x                          Decimal?                           @db.Decimal
  focal_y                          Decimal?                           @db.Decimal
  convert_webp                     Boolean?                           @default(false)
  jobs_blocks_image_block          jobs_blocks_image_block[]
  navigation_menu                  navigation_menu[]
  pages                            pages[]
  pages_blocks_image_block         pages_blocks_image_block[]
  payload_locked_documents_rels    payload_locked_documents_rels[]
  project_pages                    project_pages[]
  project_pages_blocks_image_block project_pages_blocks_image_block[]
  projects                         projects[]
  skills                           skills[]
  testimonials                     testimonials[]

  @@index([created_at])
  @@index([updated_at])
}

model nav_links {
  id                            Int                             @id @default(autoincrement())
  label                         String                          @db.VarChar
  link                          String                          @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  navigation_menu_rels          navigation_menu_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model navigation_menu {
  id                            Int                             @id @default(autoincrement())
  title                         String                          @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  logo_id                       Int?
  media                         media?                          @relation(fields: [logo_id], references: [id], onUpdate: NoAction, map: "navigation_menu_logo_id_media_id_fk")
  navigation_menu_rels          navigation_menu_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([logo_id], map: "navigation_menu_logo_idx")
  @@index([updated_at])
}

model navigation_menu_rels {
  id              Int             @id @default(autoincrement())
  order           Int?
  parent_id       Int
  path            String          @db.VarChar
  nav_links_id    Int?
  social_links_id Int?
  nav_links       nav_links?      @relation(fields: [nav_links_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "navigation_menu_rels_nav_links_fk")
  navigation_menu navigation_menu @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "navigation_menu_rels_parent_fk")
  social_links    social_links?   @relation(fields: [social_links_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "navigation_menu_rels_social_links_fk")

  @@index([nav_links_id])
  @@index([order])
  @@index([parent_id], map: "navigation_menu_rels_parent_idx")
  @@index([path])
  @@index([social_links_id])
}

model pages {
  id                                     Int                                      @id @default(autoincrement())
  title                                  String                                   @db.VarChar
  updated_at                             DateTime                                 @default(now()) @db.Timestamptz(3)
  created_at                             DateTime                                 @default(now()) @db.Timestamptz(3)
  meta_title                             String?                                  @db.VarChar
  meta_description                       String?                                  @db.VarChar
  meta_image_id                          Int?
  slug                                   String?                                  @db.VarChar
  nested_route                           enum_pages_nested_route
  media                                  media?                                   @relation(fields: [meta_image_id], references: [id], onUpdate: NoAction, map: "pages_meta_image_id_media_id_fk")
  pages_blocks_call_to_action            pages_blocks_call_to_action[]
  pages_blocks_card                      pages_blocks_card[]
  pages_blocks_code_mockup_link          pages_blocks_code_mockup_link[]
  pages_blocks_code_mockup_section       pages_blocks_code_mockup_section[]
  pages_blocks_hero_section              pages_blocks_hero_section[]
  pages_blocks_image_block               pages_blocks_image_block[]
  pages_blocks_in_view_basic             pages_blocks_in_view_basic[]
  pages_blocks_in_view_embed             pages_blocks_in_view_embed[]
  pages_blocks_job_section_block         pages_blocks_job_section_block[]
  pages_blocks_rich_text_block           pages_blocks_rich_text_block[]
  pages_blocks_skills_section            pages_blocks_skills_section[]
  pages_blocks_testimonial_section_block pages_blocks_testimonial_section_block[]
  pages_blocks_two_column_grid           pages_blocks_two_column_grid[]
  pages_rels_pages_rels_pages_idTopages  pages_rels[]                             @relation("pages_rels_pages_idTopages")
  pages_rels_pages_rels_parent_idTopages pages_rels[]                             @relation("pages_rels_parent_idTopages")
  payload_locked_documents_rels          payload_locked_documents_rels[]
  project_pages_rels                     project_pages_rels[]

  @@index([created_at])
  @@index([meta_image_id], map: "pages_meta_meta_image_idx")
  @@index([updated_at])
}

model pages_blocks_call_to_action {
  order           Int                                               @map("_order")
  parent_id       Int                                               @map("_parent_id")
  path            String                                            @map("_path")
  id              String                                            @id @db.VarChar
  style           enum_pages_blocks_call_to_action_style?
  link_type       enum_pages_blocks_call_to_action_link_type?       @default(reference)
  link_new_tab    Boolean?                                          @default(false)
  link_url        String?                                           @db.VarChar
  link_label      String                                            @db.VarChar
  arrow           Boolean?                                          @default(false)
  arrow_direction enum_pages_blocks_call_to_action_arrow_direction?
  block_name      String?                                           @db.VarChar
  pages           pages                                             @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_call_to_action_parent_id_fk")

  @@index([order], map: "pages_blocks_call_to_action_order_idx")
  @@index([parent_id], map: "pages_blocks_call_to_action_parent_id_idx")
  @@index([path], map: "pages_blocks_call_to_action_path_idx")
}

model pages_blocks_card {
  order      Int     @map("_order")
  parent_id  Int     @map("_parent_id")
  path       String  @map("_path")
  id         String  @id @db.VarChar
  pill       String? @db.VarChar
  block_name String? @db.VarChar
  pages      pages   @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_card_parent_id_fk")

  @@index([order], map: "pages_blocks_card_order_idx")
  @@index([parent_id], map: "pages_blocks_card_parent_id_idx")
  @@index([path], map: "pages_blocks_card_path_idx")
}

model pages_blocks_code_mockup_link {
  order      Int                                           @map("_order")
  parent_id  Int                                           @map("_parent_id")
  path       String                                        @map("_path")
  id         String                                        @id @db.VarChar
  prefix     String?                                       @db.VarChar
  text       String                                        @db.VarChar
  text_color enum_pages_blocks_code_mockup_link_text_color
  block_name String?                                       @db.VarChar
  pages      pages                                         @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_code_mockup_link_parent_id_fk")

  @@index([order], map: "pages_blocks_code_mockup_link_order_idx")
  @@index([parent_id], map: "pages_blocks_code_mockup_link_parent_id_idx")
  @@index([path], map: "pages_blocks_code_mockup_link_path_idx")
}

model pages_blocks_code_mockup_section {
  order           Int      @map("_order")
  parent_id       Int      @map("_parent_id")
  path            String   @map("_path")
  id              String   @id @db.VarChar
  enable_section  Boolean?
  block_name      String?  @db.VarChar
  use_random_data Boolean?
  pages           pages    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_code_mockup_section_parent_id_fk")

  @@index([order], map: "pages_blocks_code_mockup_section_order_idx")
  @@index([parent_id], map: "pages_blocks_code_mockup_section_parent_id_idx")
  @@index([path], map: "pages_blocks_code_mockup_section_path_idx")
}

model pages_blocks_hero_section {
  order                  Int      @map("_order")
  parent_id              Int      @map("_parent_id")
  path                   String   @map("_path")
  id                     String   @id @db.VarChar
  title                  String   @db.VarChar
  subtitle               String?  @db.VarChar
  description            String?  @db.VarChar
  block_name             String?  @db.VarChar
  section_id             String   @db.VarChar
  enable_inner_container Boolean? @default(false)
  secondary_blurb        String?  @db.VarChar
  grey_background        Boolean?
  pages                  pages    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_hero_section_parent_id_fk")

  @@index([order], map: "pages_blocks_hero_section_order_idx")
  @@index([parent_id], map: "pages_blocks_hero_section_parent_id_idx")
  @@index([path], map: "pages_blocks_hero_section_path_idx")
}

model pages_blocks_image_block {
  order           Int                                            @map("_order")
  parent_id       Int                                            @map("_parent_id")
  path            String                                         @map("_path")
  id              String                                         @id @db.VarChar
  image_id        Int
  gradient        Boolean?                                       @default(false)
  gradient_x_flip Boolean?                                       @default(false)
  gradient_y_flip Boolean?                                       @default(false)
  gradient_select enum_pages_blocks_image_block_gradient_select?
  block_name      String?                                        @db.VarChar
  forced_width    Decimal                                        @default(0) @db.Decimal
  forced_height   Decimal                                        @default(0) @db.Decimal
  media           media                                          @relation(fields: [image_id], references: [id], onDelete: SetNull, onUpdate: NoAction, map: "pages_blocks_image_block_image_id_media_id_fk")
  pages           pages                                          @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_image_block_parent_id_fk")

  @@index([image_id], map: "pages_blocks_image_block_image_idx")
  @@index([order], map: "pages_blocks_image_block_order_idx")
  @@index([parent_id], map: "pages_blocks_image_block_parent_id_idx")
  @@index([path], map: "pages_blocks_image_block_path_idx")
}

model pages_blocks_in_view_basic {
  order        Int      @map("_order")
  parent_id    Int      @map("_parent_id")
  path         String   @map("_path")
  id           String   @id @db.VarChar
  title        String   @db.VarChar
  description  String   @db.VarChar
  hidden_y     Decimal? @default(0) @db.Decimal
  hidden_blur  Decimal? @default(4) @db.Decimal
  visible_y    Decimal? @default(1) @db.Decimal
  visible_blur Decimal? @default(0) @db.Decimal
  block_name   String?  @db.VarChar
  pages        pages    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_in_view_basic_parent_id_fk")

  @@index([order], map: "pages_blocks_in_view_basic_order_idx")
  @@index([parent_id], map: "pages_blocks_in_view_basic_parent_id_idx")
  @@index([path], map: "pages_blocks_in_view_basic_path_idx")
}

model pages_blocks_in_view_embed {
  order        Int      @map("_order")
  parent_id    Int      @map("_parent_id")
  path         String   @map("_path")
  id           String   @id @db.VarChar
  section_id   String   @db.VarChar
  hidden_y     Decimal? @default(0) @db.Decimal
  hidden_blur  Decimal? @default(4) @db.Decimal
  visible_y    Decimal? @default(1) @db.Decimal
  visible_blur Decimal? @default(0) @db.Decimal
  block_name   String?  @db.VarChar
  pages        pages    @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_in_view_embed_parent_id_fk")

  @@index([order], map: "pages_blocks_in_view_embed_order_idx")
  @@index([parent_id], map: "pages_blocks_in_view_embed_parent_id_idx")
  @@index([path], map: "pages_blocks_in_view_embed_path_idx")
}

model pages_blocks_job_section_block {
  order           Int                                                  @map("_order")
  parent_id       Int                                                  @map("_parent_id")
  path            String                                               @map("_path")
  id              String                                               @id @db.VarChar
  title           String                                               @db.VarChar
  gradient        Boolean?                                             @default(false)
  gradient_select enum_pages_blocks_job_section_block_gradient_select?
  block_name      String?                                              @db.VarChar
  section_id      String                                               @db.VarChar
  pages           pages                                                @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_job_section_block_parent_id_fk")

  @@index([order], map: "pages_blocks_job_section_block_order_idx")
  @@index([parent_id], map: "pages_blocks_job_section_block_parent_id_idx")
  @@index([path], map: "pages_blocks_job_section_block_path_idx")
}

model pages_blocks_rich_text_block {
  order        Int     @map("_order")
  parent_id    Int     @map("_parent_id")
  path         String  @map("_path")
  id           String  @id @db.VarChar
  block_name   String? @db.VarChar
  content      Json
  content_html String? @db.VarChar
  pages        pages   @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_rich_text_block_parent_id_fk")

  @@index([order], map: "pages_blocks_rich_text_block_order_idx")
  @@index([parent_id], map: "pages_blocks_rich_text_block_parent_id_idx")
  @@index([path], map: "pages_blocks_rich_text_block_path_idx")
}

model pages_blocks_skills_section {
  order                Int                                               @map("_order")
  parent_id            Int                                               @map("_parent_id")
  path                 String                                            @map("_path")
  id                   String                                            @id @db.VarChar
  section_id           String                                            @db.VarChar
  skills_collection_id Int
  block_name           String?                                           @db.VarChar
  heading              String                                            @db.VarChar
  gradient             Boolean?                                          @default(false)
  gradient_select      enum_pages_blocks_skills_section_gradient_select?
  pill                 String?                                           @db.VarChar
  description          String?                                           @db.VarChar
  pages                pages                                             @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_skills_section_parent_id_fk")
  skills_collection    skills_collection                                 @relation(fields: [skills_collection_id], references: [id], onDelete: SetNull, onUpdate: NoAction, map: "pages_blocks_skills_section_skills_collection_id_skills_collect")

  @@index([order], map: "pages_blocks_skills_section_order_idx")
  @@index([parent_id], map: "pages_blocks_skills_section_parent_id_idx")
  @@index([path], map: "pages_blocks_skills_section_path_idx")
  @@index([skills_collection_id], map: "pages_blocks_skills_section_skills_collection_idx")
}

model pages_blocks_testimonial_section_block {
  order           Int                                                          @map("_order")
  parent_id       Int                                                          @map("_parent_id")
  path            String                                                       @map("_path")
  id              String                                                       @id @db.VarChar
  section_id      String?                                                      @db.VarChar
  pill            String?                                                      @db.VarChar
  heading         String                                                       @db.VarChar
  description     String?                                                      @db.VarChar
  block_name      String?                                                      @db.VarChar
  gradient        Boolean?                                                     @default(false)
  gradient_select enum_pages_blocks_testimonial_section_block_gradient_select?
  carousel_custom Boolean?                                                     @default(false)
  enable_infinite Boolean?                                                     @default(false)
  carousel_timer  Boolean?                                                     @default(false)
  seconds_timer   Decimal?                                                     @db.Decimal
  pages           pages                                                        @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "pages_blocks_testimonial_section_block_parent_id_fk")

  @@index([order], map: "pages_blocks_testimonial_section_block_order_idx")
  @@index([parent_id], map: "pages_blocks_testimonial_section_block_parent_id_idx")
  @@index([path], map: "pages_blocks_testimonial_section_block_path_idx")
