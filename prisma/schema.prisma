  media                         media                           @relation(fields: [skill_icon_id], references: [id], onDelete: SetNull, onUpdate: NoAction, map: "skills_skill_icon_id_media_id_fk")
  skills_collection_rels        skills_collection_rels[]

  @@index([created_at])
  @@index([skill_icon_id], map: "skills_skill_icon_idx")
  @@index([updated_at])
}

model skills_collection {
  id                            Int                             @id @default(autoincrement())
  title                         String                          @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  pages_blocks_skills_section   pages_blocks_skills_section[]
  payload_locked_documents_rels payload_locked_documents_rels[]
  skills_collection_rels        skills_collection_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model skills_collection_rels {
  id                Int               @id @default(autoincrement())
  order             Int?
  parent_id         Int
  path              String            @db.VarChar
  skills_id         Int?
  skills_collection skills_collection @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "skills_collection_rels_parent_fk")
  skills            skills?           @relation(fields: [skills_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "skills_collection_rels_skills_fk")

  @@index([order])
  @@index([parent_id], map: "skills_collection_rels_parent_idx")
  @@index([path])
  @@index([skills_id])
}

model social_links {
  id                            Int                             @id @default(autoincrement())
  label                         String                          @db.VarChar
  link                          String                          @db.VarChar
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  navigation_menu_rels          navigation_menu_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model testimonials {
  id                            Int                             @id @default(autoincrement())
  name                          String                          @db.VarChar
  position                      String                          @db.VarChar
  company                       String                          @db.VarChar
  headshot_id                   Int
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  content                       Json
  content_html                  String?                         @db.VarChar
  callout                       String                          @db.VarChar
  pages_rels                    pages_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]
  media                         media                           @relation(fields: [headshot_id], references: [id], onDelete: SetNull, onUpdate: NoAction, map: "testimonials_headshot_id_media_id_fk")

  @@index([created_at])
  @@index([headshot_id], map: "testimonials_headshot_idx")
  @@index([updated_at])
}

model users {
  id                            Int                             @id @default(autoincrement())
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  email                         String                          @unique(map: "users_email_idx") @db.VarChar
  reset_password_token          String?                         @db.VarChar
  reset_password_expiration     DateTime?                       @db.Timestamptz(3)
  salt                          String?                         @db.VarChar
  hash                          String?                         @db.VarChar
  login_attempts                Decimal?                        @default(0) @db.Decimal
  lock_until                    DateTime?                       @db.Timestamptz(3)
  enable_a_p_i_key              Boolean?
  api_key                       String?                         @db.VarChar
  api_key_index                 String?                         @db.VarChar
  payload_locked_documents_rels payload_locked_documents_rels[]
  payload_preferences_rels      payload_preferences_rels[]

  @@index([created_at])
  @@index([updated_at])
}

